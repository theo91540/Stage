<template>
<name>M_t</name>
<parameter>urgent chan &amp;lock_M, urgent chan &amp;unlock_M</parameter>
<declaration></declaration>
<location id="23" x="0" y="0"><name x="-20" y="-25">W</name></location>
<location id="24" x="150" y="0"></location>
<init ref="23"/>
<transition><source ref="23"/><target ref="24"/><label kind="synchronisation" x="50" y="-20">lock_M?</label></transition>
<transition><source ref="24"/><target ref="23"/><label kind="synchronisation" x="50" y="80">unlock_M?</label><nail x="150" y="100"/><nail x="0" y="100"/></transition>
</template>
<system>
urgent chan k_S1_F;
urgent chan k_S2_F;
urgent chan k_S2_B;
urgent chan k_F_B;
urgent chan k_F_P;
urgent chan k_B_P;
urgent chan lock_M;
urgent chan unlock_M;

S1 = S1_t(k_S1_F);
S2 = S2_t(k_S2_F, k_S2_B);
F = F_t(k_S1_F, k_S2_F, k_F_B, k_F_P);
B = B_t(k_S2_B, k_F_B, k_B_P);
P = P_t(k_F_P, k_B_P, lock_M, unlock_M);
M = M_t(lock_M, unlock_M);

system S1, S2, F, B, P, M;
</system>
</nta>
