<transition><source ref="16"/><target ref="18"/><label kind="synchronisation" x="-50" y="-60">k_B_P?</label></transition>
<transition><source ref="18"/><target ref="19"/><label kind="synchronisation" x="50" y="-120">k_F_P?</label><label kind="assignment" x="200" y="10">x=0</label></transition>
<transition><source ref="17"/><target ref="20"/><label kind="guard" x="200" y="-20">x&gt;=250</label></transition>
