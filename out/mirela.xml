<parameter>urgent chan &amp;k_F_P, urgent chan &amp;k_B_P, urgent chan &amp;lock_M, urgent chan &amp;unlock_M</parameter><declaration>clock x;</declaration>
<location id="16" x="0" y="0"><name x="-20" y="-25">W</name></location>
<location id="17" x="150" y="0"><name x="130" y="-25">P</name><label kind="invariant" x="100" y="20">x&lt;300</label></location>
<location id="18" x="0" y="-100"><name x="-20" y="-125">W2</name></location>
<location id="19" x="150" y="-100"><name x="130" y="-125">P2</name><label kind="invariant" x="100" y="-80">x&lt;300</label></location>
<location id="20" x="300" y="0"></location>
