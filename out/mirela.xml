<transition><source ref="20"/><target ref="21"/><label kind="synchronisation" x="350" y="80">lock_M!</label><label kind="assignment" x="350" y="110">x=0</label><nail x="450" y="0"/><nail x="450" y="100"/></transition>
<transition><source ref="21"/><target ref="22"/><label kind="guard" x="200" y="80">x&gt;=20</label></transition>
